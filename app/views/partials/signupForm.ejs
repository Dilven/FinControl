<div class="cont">
    <div class="demo">
        <div class="login">
            <div class="login__check"></div>
            <form action="/register" method="post" class="login__form">
            <div class="login__row">
                <svg class="login__icon name svg-icon" viewBox="0 0 20 20">
                <path d="M0,20 a10,8 0 0,1 20,0z M10,0 a4,4 0 0,1 0,8 a4,4 0 0,1 0,-8" />
                </svg>
                <input name="email" type="text" class="login__input name" placeholder="Email" for="signup-email" id="signin-email"/>
            </div>
            <div class="login__row">
                <svg class="login__icon pass svg-icon" viewBox="0 0 20 20">
                <path d="M0,20 20,20 20,8 0,8z M10,13 10,16z M4,8 a6,8 0 0,1 12,0" />
                </svg>
                <input name="password" type="password" class="login__input pass" placeholder="Hasło" for="signup-password" id="signin-password"/>
            </div>
            <button type="submit" class="login__submit">Zaloguj się</button>
            <p class="login__signup">Masz juz konto? <a href="/login">Zaloguj się</a></p>
            </form>
        </div>
    </div>
</div>